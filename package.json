{
  "name": "@openstaticfish/bouncer",
  "version": "0.1.0",
  "description": "TUI for managing GitHub branch protection rules",
  "author": "OpenStaticFish",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenStaticFish/bouncer.git"
  },
  "homepage": "https://github.com/OpenStaticFish/bouncer#readme",
  "bugs": {
    "url": "https://github.com/OpenStaticFish/bouncer/issues"
  },
  "keywords": [
    "github",
    "branch-protection",
    "cli",
    "tui",
    "devops",
    "terminal",
    "git"
  ],
  "type": "module",
  "bin": {
    "bouncer": "./cli.ts"
  },
  "main": "./cli.ts",
  "files": [
    "cli.ts",
    "src/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "start": "bun run cli.ts",
    "start:local": "bun run cli.ts --local",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "prepack": "bun run typecheck && bun test"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@opentui/core": "^0.1.79"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
